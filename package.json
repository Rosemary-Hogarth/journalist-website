    "cross-env": "^7.0.3",
    "cssnano": "^5.0.15",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "sass": "^1.80.6"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-bundle": "^3.0.0",
    "bootstrap": "^5.3.3",
